{
    "version": 2,
    "builds": [
      {
        "src": "api/index.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "api/index.js",
        "methods": ["GET", "POST", "PUT", "DELETE", "OPTIONS"]
      }
    ],
    "env": {
      "NODE_ENV": "production"
    }
  }